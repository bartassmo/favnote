{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "env": {
      "jest": true
  },
  "globals": {
      "document": true
  },
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js"]
      }
    ],
    "react/jsx-indent": [
      1,
      {
        "tab": [4]
      }
    ],
    "react/jsx-indent-props": [
      1,
      {
        "tab": [2]
      }
    ]
  }
}
